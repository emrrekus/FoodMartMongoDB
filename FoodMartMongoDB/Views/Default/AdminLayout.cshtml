@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
@await Component.InvokeAsync("_AdminHeadLayoutComponentPartial")
<body>
    <div class="wrapper">

        @await Component.InvokeAsync("_AdminSidebarComponentPartial")
        @await Component.InvokeAsync("_AdminNavbarLayoutComponentPartial")

        <div class="container">
            <div class="page-inner">

                @RenderBody()
                
            </div>
        </div>
        @await Component.InvokeAsync("_AdminFooterLayoutComponentPartial")


    </div>

    @await Component.InvokeAsync("_AdminJavaScriptsLayoutComponentPartial")
</body>
</html>
